<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Opgave12._4.MainPage">

    <VerticalStackLayout>
        <CollectionView x:Name="BilCollectionView" >
            <CollectionView.ItemTemplate>
                <DataTemplate>
                    <VerticalStackLayout>
                        <Label x:Name="ModelLabel" Text="{Binding Path=Model}"></Label>
                       
                    </VerticalStackLayout>

                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>


        <Button x:Name="TilFoejBilButton" Text="Tilføj Bil" Clicked="OnTilfoejBilClicked"/>
        <Button x:Name="RetBilBUtton" Text="Ret bil" Clicked="OnRetBilClickked"/>

    </VerticalStackLayout>

</ContentPage>
